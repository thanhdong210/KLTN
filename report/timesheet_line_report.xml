<odoo>

    <template id="report_timesheet_line">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Report title</h2>
                        <p>This object's name is <span t-field="o.name"/></p>
                        <t t-foreach="o.timesheet_line_ids" t-as="line">
                            <span t-field="line.number_of_days"/>
                        </t>
                    </div>
                </t>
            </t>
        </t>
    </template>

    <record id="action_timesheet_line_report" model="ir.actions.report">
        <field name="name">Timesheet Line Report</field>
        <field name="model">hr.timesheet</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">KLTN.report_timesheet_line</field>
        <field name="report_file">KLTN.report_timesheet_line</field>
        <field name="print_report_name">'Timesheet Report - %s' % (object.name)</field>
        <field name="binding_model_id" ref="model_hr_timesheet"/>
    </record>

</odoo>