    <menuitem id="employee_dashboard_menu"
        name="Human Resource"
        web_icon="kltn,static/description/icon.png"
        action="employee_dashboard_view"
        groups="base.group_user" />

    <menuitem id="personal_dashboard_menu"
        name="Personal Hub"
        web_icon="kltn,static/description/personal_hub.png"
