<odoo>

    <record id="employee_dashboard_view" model="ir.actions.client">
        <field name="name">Dashboard</field>
        <field name="tag">hr_dashboard</field>
    </record>

    <record id="attendance_data_view" model="ir.actions.client">
        <field name="name">Attendance Data</field>
        <field name="tag">hr_attendance</field>
    </record>

    <record id="chart_view" model="ir.actions.client">
        <field name="name">Chart</field>
        <field name="tag">hr_chart</field>
